io

# basic setup
set FKEY 1
set GVAL 1.0

# adaptivity
set ETOL 3.0e-1
set AKEY 2
set BKEY 1
set RKEY 0
set RCOL -1

# solver
set NKEY 0
set LKEY 0
set GUES 2
set LMAX 5000
set NMAX 20
set LTOL 1.0e-7

# write/plot
set FKEY 1
set GVAL 1.0

##############################################################################
# initialization
##############################################################################

# read-plotmesh-solve-plotsol-pause
read
set OSNAM 0
writeoff
lsolve
set OSNAM 1
writeoffdis
pause

